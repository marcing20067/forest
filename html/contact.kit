<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Kontakt do firmy Forest">
    <title>Forest - Kontakt</title>
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="./dist/css/contact.min.css">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="./dist/css/hamburger.min.css">
</head>

<body>
    <!-- @include nav.kit -->
    <div class="content">
        <div class="nav-margin"></div>
        <div class="wrapper-smaller">
            <header>
                <h1 class="h1 title">Forest - Kontakt</h1>
            </header>
        </div>
        <main>
            <div class="wrapper-smaller">
                <div class="box">
                    <form class="form">
                        <div class="form__box">
                            <input class="form__box-input" id="name" type="text" required>
                            <label class="form__box-label" for="name">Imię:</label>
                            <div class="form__box-bar"></div>
                        </div>

                        <div class="form__box">
                            <input class="form__box-input" id="email" type="email" required>
                            <label class="form__box-label" for="email">Email:</label>
                            <div class="form__box-bar"></div>
                        </div>

                        <div class="form__box">
                            <input class="form__box-input" id="topic" type="text" required>
                            <label class="form__box-label" for="topic">Temat:</label>
                            <div class="form__box-bar"></div>
                        </div>

                        <div class="form__box">
                            <textarea class="form__box-input form__box-textarea" id="content" type="text"
                                required></textarea>
                            <label class="form__box-label" for="content">Treść wiadomości:</label>
                            <div class="form__box-bar"></div>
                        </div>
                        <button href="offers.html" type="submit" class="form__btn-submit button">Wyślij</button>
                    </form>

                    <iframe
                        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.8952530760016!2d19.890611315612585!3d50.088247979426995!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sMcDonald&#39;s!5e0!3m2!1sen!2spl!4v1612519945713!5m2!1sen!2spl"
                        class="map-frame" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false"
                        tabindex="0" title="Mapa Google McDonald's Jasnogórska 2, 31-342 Kraków"></iframe>
                </div>
            </div>
        </main>
    </div>
    <!-- @include footer.kit -->

    <script src="./dist/js/main.min.js"></script>
</body>

</html>